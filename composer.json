{
    "name": "distilleries/security",
    "description": "Security Middleweare and Helper",
    "license": "MIT",
    "keywords": [
        "framework",
        "laravel",
        "admin",
        "Security",
        "distilleries",
        "security"
    ],
    "authors": [
        {
            "name": "Maxime Francois",
            "email": "kezho@me.com"
        }
    ],

    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "~5.7|~5.8|~6.0",
        "ezyang/htmlpurifier": "^4.8"
    },
    "require-dev": {
        "orchestra/testbench": "~3.7|~3.8|^4.0",
        "orchestra/testbench-browser-kit": "~3.7|~3.8|^4.0",
        "phpunit/phpunit": "~7.0|^8.3",
        "mockery/mockery": "~1.0|^1.2.3",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "Distilleries\\Security\\": "src/Distilleries/Security"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Distilleries\\Security\\SecurityServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
